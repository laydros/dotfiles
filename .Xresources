! vim:ts=8
! $OpenBSD: dot.Xdefaults,v 1.3 2014/07/10 10:22:59 jasper Exp $
!
Xcursor.theme: Qogir
Xcursor.size: 16

*.saveLines:                    20000

XTerm*loginShell:               true

!XTerm*foreground:               white
!XTerm*background:               black
XTerm*metaSendsEscape:          true
!XTerm*faceName:                 spleen:pixelsize=18:antialias=false
!XTerm*faceName:                 spleen:pixelsize=14:antialias=false
XTerm*geometry:                 90x29

URxvt.font:xft:agave Nerd Font:pixelsize=14, xft:DejaVu Sans Mono Nerd Font:pixelsize=14

! == XCLOCK
XClock*analog:                  false
XClock*geometry:                -2-2

#define BG                      #d9d5ba
#define FG                      #444444
*.background:                   BG
*.foreground:                   FG

! black
*.color0:			FG
*.color8:			#999

! red
*.color1:			#825b69
*.color9:			#bda0aa

! green
*.color2:			#69825b
*.color10:			#aabda0

! yellow
*.color3:			#82755b
*.color11:			#bdb3a0

! blue
!*.color4:			#5b6982
*.color4:                       #6090d0
*.color12:			#a0aabd

! magenta
*.color5:			#755b82
*.color13:			#b3a0bd

! cyan
*.color6:			#5b8275
*.color14:			#a0bdb3

! white (gray)
*.color7:			#a1d7cc
*.color15:			#e5e5e5


XTerm.rightScrollBar:		true
URxvt.scrollBar_right:		true
URxvt.scrollstyle:		xterm
*.selectToClipboard:		true
*.termName:			xterm-256color
